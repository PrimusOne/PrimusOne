            <set name="rarity">Very Rare</set>
            <set name="attunement">true</set>
            <set name="weapon">Dagger</set>
        </setters>
    </element>

    <element name="Cursed/Blessed Coin" type="Magic Item" source="Primus Two" id="ID_PT_MAGIC_ITEM_CURSED_BLESSED_COIN">
        <description>
            <p>Diese magische Münze verleiht dir im richtigen Moment das richtige/falsche Ergebnis. Vor einem Attackroll (2x/Longrest) kannst du diese Münze flippen. Bei Kopf (gerade) wird der Angriff ein Krit, bei Zahl (ungerade) passiert etwas Schlimmes...</p>
        </description>
        <setters>
            <set name="category">Wondrous Items</set>
            <set name="cost" currency="gp">0</set>
            <set name="weight" lb="0">0 lb.</set>
            <set name="type">Wondrous Items</set>
            <set name="rarity">Artifact</set>
            <set name="attunement">true</set>
        </setters>
    </element>

