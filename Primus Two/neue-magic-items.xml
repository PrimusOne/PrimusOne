        <setters>
            <set name="category">Magic Armor</set>
            <set name="cost" currency="gp">0</set>
            <set name="slot">body</set>
            <set name="type" addition="equippable by a cleric or paladin">Armor</set>
            <set name="rarity">Rare</set>
            <set name="enhancement">1</set>
            <set name="armor">ID_INTERNAL_ARMOR_GROUP_MEDIUM|ID_INTERNAL_ARMOR_GROUP_HEAVY</set>
        </setters>
        <rules>
            <stat name="ac:armored:enhancement" value="1" bonus="enhancement" />
            <stat name="strength:save:misc" value="1" />
            <stat name="dexterity:save:misc" value="1" />
            <stat name="constitution:save:misc" value="1" />
            <stat name="intelligence:save:misc" value="1" />
            <stat name="wisdom:save:misc" value="1" />
            <stat name="charisma:save:misc" value="1" />
            <select type="Class Feature" name="Divine Protector" supports="Resistanc"/>

        </rules>
    </element>

    <element name="Deathseeker" type="Magic Item" source="Primus Two" id="ID_PT_MAGIC_ITEM_DEATHSEEKER">
        <description>
            <p>Dieser magische dunkle Dagger besitzt eine ominöse Aura, die Gegner verunsichert im Kampf. Die Kritchance steigt um +2 und der to-hit sinkt um -5.</p>
        </description>
